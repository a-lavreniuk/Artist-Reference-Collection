{
  "manifest_version": 3,
  "name": "Save to ARC",
  "version": "1.0",
  "description": "Сохраняйте изображения в ARC - Artist Reference Collection",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}

